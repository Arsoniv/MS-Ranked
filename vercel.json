{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/game.js",
      "dest": "/public/game.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ."
}
