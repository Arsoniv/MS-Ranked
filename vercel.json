{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/play/(.*)", "dest": "/public/play/$1" },
    { "src": "/(.*)", "dest": "/public/home/$1" },
    { "src": "/prac/(.*)", "dest": "/public/practice/$1" }
  ]
}
